<keymap version="1" name="Default copy" parent="$default">
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="shift ctrl x" />
  </action>
  <action id="Scala.SendSelectionToConsole" />
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="SelectInProjectView">
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
</keymap>