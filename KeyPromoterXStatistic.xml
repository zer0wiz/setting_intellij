<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug 'AppMain'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⇧F9'</ShortCut>
          <Description>Debug 'AppMain'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'MountainBatchApplication'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⇧F9'</ShortCut>
          <Description>Debug 'MountainBatchApplication'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'tomcat'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⇧F9'</ShortCut>
          <Description>Debug 'tomcat'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>GoTo</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Highlight current scope">
        <Item>
          <IdeaActionID>Rainbow.ScopeHighlightingAction</IdeaActionID>
          <ShortCut>'⌘Button3 Click'</ShortCut>
          <Description>Highlight current scope</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Implementation(s)">
        <Item>
          <IdeaActionID>GotoImplementation</IdeaActionID>
          <ShortCut>'⌥⌘B' or '⌥⌘Button1 Click'</ShortCut>
          <Description>Implementation(s)</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Library Settings">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'F4'</ShortCut>
          <Description>Open Library Settings</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'AppMain'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⇧F10'</ShortCut>
          <Description>Run 'AppMain'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diff">
        <Item>
          <IdeaActionID>Diff.ShowDiff</IdeaActionID>
          <ShortCut>'⌘D'</ShortCut>
          <Description>Show Diff</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'MountainBatchApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'MountainBatchApplication'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'local'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'local'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'tomcat'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'tomcat'</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Vim Emulator">
        <Item>
          <IdeaActionID>VimPluginToggle</IdeaActionID>
          <ShortCut>'⌥⌘V'</ShortCut>
          <Description>Vim Emulator</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>