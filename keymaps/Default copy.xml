<keymap version="1" name="Default copy" parent="$default">
  <action id="EmmetPreviousEditPoint">
    <keyboard-shortcut first-keystroke="shift alt open_bracket" />
    <keyboard-shortcut first-keystroke="shift alt open_bracket" second-keystroke="shift ctrl comma" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
</keymap>